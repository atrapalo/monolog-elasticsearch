{
    "name": "atrapalo/monolog-elasticsearch",
    "description": "A Monolog handler and formatter that makes use of the elasticsearch/elasticsearch package",
    "type": "library",
    "keywords": ["log", "logging", "logger", "monolog", "psr3", "psr-3", "elastic", "elasticsearch"],
    "require": {
        "elasticsearch/elasticsearch": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "monolog/monolog": "^1.17"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Soronellas",
            "email": "christian.soronellas@atrapalo.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "bin-dir": "bin/"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "tests/"
        }
    }
}
